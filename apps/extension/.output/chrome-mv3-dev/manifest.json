{
  "manifest_version": 3,
  "name": "@echotab/extension",
  "description": "An open-source Chrome tab manager",
  "version": "0.0.25",
  "key": "",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "bookmarks",
    "favicon",
    "activeTab",
    "contextMenus",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>",
    "http://localhost/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "newtab.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "<all_urls>"
    ]
  },
  "commands": {
    "wxt:reload-extension": {
      "description": "Reload the extension during development",
      "suggested_key": {
        "default": "Alt+R"
      }
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval' http://localhost:3000; object-src 'self';",
    "sandbox": "script-src 'self' 'unsafe-inline' 'unsafe-eval' http://localhost:3000; sandbox allow-scripts allow-forms allow-popups allow-modals; child-src 'self';"
  }
}