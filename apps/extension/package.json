{
  "name": "@echotab/extension",
  "displayName": "EchoTab - Extension",
  "version": "0.0.23",
  "description": "An open-source Chrome tab manager",
  "author": "Buka Studio",
  "scripts": {
    "dev": "plasmo dev --verbose",
    "build": "plasmo build",
    "lint": "eslint src",
    "package": "plasmo package"
  },
  "dependencies": {
    "@dnd-kit/core": "~6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "~10.0.0",
    "@dnd-kit/utilities": "~3.2.2",
    "@echotab/lists": "*",
    "@echotab/ui": "*",
    "@hookform/resolvers": "~3.10.0",
    "@tanstack/react-query": "^5.65.1",
    "@tanstack/react-virtual": "~3.11.3",
    "@viselect/react": "~3.5.1",
    "@visx/chord": "^3.3.0",
    "@visx/visx": "~3.12.0",
    "chroma-js": "^3.1.2",
    "class-variance-authority": "^0.7.1",
    "cmdk": "~1.0.4",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "derive-valtio": "~0.2.0",
    "framer-motion": "^12.0.6",
    "fuse.js": "~7.0.0",
    "idb": "~8.0.1",
    "openai": "~4.81.0",
    "plasmo": "0.89.4",
    "react": "^18",
    "react-dom": "^18",
    "react-error-boundary": "~5.0.0",
    "react-frame-component": "~5.2.7",
    "react-hook-form": "~7.54.2",
    "react-hotkeys-hook": "~4.6.1",
    "tweakpane": "^4.0.5",
    "uuidv7": "^1.0.2",
    "valtio": "~2.1.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@echotab/eslint": "*",
    "@echotab/tsconfig": "*",
    "@eslint/js": "^9.19.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tweakpane/core": "^2.0.5",
    "@types/chroma-js": "^3.1.1",
    "@types/chrome": "0.0.299",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "22.12.0",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "autoprefixer": "~10.4.20",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-valtio": "^0.8.0",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  },
  "manifest": {
    "key": "",
    "permissions": [
      "storage",
      "unlimitedStorage",
      "bookmarks",
      "favicon",
      "activeTab"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "newtab.html"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "externally_connectable": {
      "matches": [
        "<all_urls>"
      ]
    }
  }
}
