{
  "name": "@echotab/db",
  "displayName": "EchoTab Database access",
  "version": "0.0.1",
  "author": "Buka Studio",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "migrate": "npx tsx src/scripts/migrate.ts",
    "gen-types": "npx kysely-codegen --include-pattern=\"public.*\" --out-file src/types.ts",
    "build": "tsc"
  },
  "private": true,
  "sideEffects": false,
  "files": [
    "src"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./connection": "./src/connection.ts"
  },
  "dependencies": {
    "@echotab/supabase": "workspace:*",
    "dotenv": "~17.2.3",
    "kysely": "^0.28.8",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@echotab/tsconfig": "workspace:*",
    "@types/pg": "^8.15.6",
    "kysely-codegen": "^0.19.0"
  }
}
